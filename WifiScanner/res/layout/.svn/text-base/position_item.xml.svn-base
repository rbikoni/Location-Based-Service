<LinearLayout android:id="@+id/LinearLayout01" android:orientation="vertical" android:layout_gravity="center_horizontal" android:gravity="center_horizontal" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10px"><TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="BSSID" android:layout_gravity="center_horizontal"></TextView>

<LinearLayout android:id="@+id/LinearLayout02" android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:layout_marginLeft="10px"><TextView android:id="@+id/TextView03" android:layout_height="wrap_content" android:text="avgLevel" android:layout_width="wrap_content"></TextView>

